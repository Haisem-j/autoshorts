{
  "name": "next-firebase-saas-kit",
  "version": "0.4.8",
  "description": "",
  "private": true,
  "sideEffects": false,
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "dev": "next dev",
    "build": "next build",
    "build:test": "NODE_ENV=test next build",
    "dev:test": "NODE_ENV=test next dev",
    "postbuild": "npm run sitemap && npm run rss && npm run blog-docs-indexer",
    "rss": "npx tsx './src/core/blog/rss-feed.ts'",
    "sitemap": "next-sitemap",
    "start": "next start",
    "start:test": "NODE_ENV=test next start",
    "typecheck": "tsc",
    "lint": "next lint",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "export": "next export",
    "healthcheck": "npm run lint && npm run typecheck",
    "firebase:emulators:start": "firebase emulators:start --project demo-makerkit --import ./firebase-seed",
    "firebase:emulators:export": "firebase emulators:export --project demo-makerkit ./firebase-seed",
    "cypress": "NODE_ENV=test cypress open",
    "cypress:headless": "NODE_ENV=test cypress run",
    "test": "firebase emulators:exec --project demo-makerkit --import ./firebase-seed \"sh ./scripts/test.sh\"",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "stripe:mock-server": "docker run --rm -it -p 12111-12112:12111-12112 stripe/stripe-mock:latest",
    "blog-docs-indexer": "npx tsx ./search-indexer",
    "killports": "sh scripts/kill-ports.sh"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.11",
    "@mdx-js/mdx": "^2.1.3",
    "@sentry/node": "^7.14.0",
    "@sentry/react": "^7.14.0",
    "@sentry/tracing": "^7.14.0",
    "@tippyjs/react": "^4.2.6",
    "classnames": "2.3.2",
    "csrf": "^3.1.0",
    "date-fns": "2.29.3",
    "feed": "^4.2.2",
    "firebase": "^9.10.0",
    "firebase-admin": "^11.0.1",
    "heroicons": "2.0.11",
    "minisearch": "^5.0.0",
    "mjml-react": "^2.0.8",
    "next": "^12.3.1",
    "next-i18next": "^12.1.0",
    "nodemailer": "^6.8.0",
    "nookies": "^2.5.2",
    "pino": "^8.6.1",
    "raw-body": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-epic-spinners": "^0.5.0",
    "react-hook-form": "^7.36.1",
    "react-hot-toast": "^2.4.0",
    "reactfire": "^4.2.2",
    "stripe": "^10.12.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.13.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@types/jest": "^29.1.1",
    "@types/mjml-react": "^2.0.6",
    "@types/node": "^18.7.23",
    "@types/nodemailer": "^6.4.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.9.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-next": "12.3.1",
    "gray-matter": "4.0.3",
    "mjml": "^4.13.0",
    "next-sitemap": "^3.1.23",
    "pino-pretty": "^9.1.0",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-highlight": "^5.0.2",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.1",
    "stripe-cli": "^0.1.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "optionalDependencies": {
    "recharts": "^2.1.14"
  }
}
