{
  "name": "next-firebase-saas-kit",
  "version": "0.0.5",
  "description": "",
  "private": true,
  "sideEffects": false,
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "dev": "next dev",
    "build": "next build",
    "dev:test": "NODE_ENV=test next dev",
    "postbuild": "npm run sitemap && npm run rss && npm run blog-docs-indexer",
    "rss": "npx tsx './src/core/blog/rss-feed.ts'",
    "sitemap": "next-sitemap",
    "start": "next start",
    "typecheck": "tsc",
    "lint": "next lint",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "export": "next export",
    "healthcheck": "npm run lint && npm run typecheck",
    "firebase:emulators:start": "firebase emulators:start --project demo-makerkit --import ./firebase-seed",
    "firebase:emulators:export": "firebase emulators:export --project demo-makerkit ./firebase-seed",
    "cypress": "NODE_ENV=test cypress open",
    "cypress:headless": "NODE_ENV=test cypress run",
    "test": "firebase emulators:exec --project demo-makerkit --import ./firebase-seed \"sh ./scripts/test.sh\"",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "stripe:mock-server": "docker run --rm -it -p 12111-12112:12111-12112 stripe/stripe-mock:latest",
    "blog-docs-indexer": "npx tsx ./search-indexer",
    "killports": "sh scripts/kill-ports.sh"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^2.0.8",
    "@mdx-js/mdx": "^2.1.3",
    "@sentry/node": "^7.11.1",
    "@sentry/react": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "@tippyjs/react": "^4.2.6",
    "classnames": "2.3.1",
    "date-fns": "2.29.2",
    "feed": "^4.2.2",
    "firebase": "^9.9.3",
    "firebase-admin": "^11.0.1",
    "heroicons": "2.0.8",
    "minisearch": "^5.0.0",
    "mjml-react": "^2.0.8",
    "next": "^12.2.5",
    "next-i18next": "^12.0.0",
    "nodemailer": "^6.7.8",
    "nookies": "^2.5.2",
    "pino": "^8.4.2",
    "pino-pretty": "^9.1.0",
    "raw-body": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-epic-spinners": "^0.5.0",
    "react-hook-form": "^7.34.2",
    "react-hot-toast": "^2.3.0",
    "reactfire": "^4.2.2",
    "sharp": "^0.30.7",
    "stripe": "^10.6.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.12.2",
    "@next/bundle-analyzer": "^12.2.5",
    "@types/jest": "^28.1.8",
    "@types/mjml-react": "^2.0.5",
    "@types/node": "^18.7.13",
    "@types/nodemailer": "^6.4.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/sharp": "^0.30.5",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.6.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.23.0",
    "eslint-config-next": "12.2.5",
    "gray-matter": "4.0.3",
    "mjml": "^4.13.0",
    "next-sitemap": "^3.1.21",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-highlight": "^5.0.2",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.1",
    "stripe-cli": "^0.1.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  }
}
