{
  "name": "next-firebase-saas-kit",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "sideEffects": false,
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npm run sitemap && npm run rss",
    "rss": "npx ts-node -O '{\"module\": \"commonjs\"}' -P ./tsconfig.json './src/core/blog/rss-feed.ts'",
    "sitemap": "next-sitemap",
    "start": "next start",
    "typecheck": "tsc",
    "lint": "next lint",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "export": "next export",
    "healthcheck": "npm run lint && npm run typecheck",
    "firebase:emulators:start": "firebase emulators:start --import ./firebase-seed",
    "firebase:emulators:export": "firebase emulators:export ./firebase-seed",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "test": "firebase emulators:exec --import ./firebase-seed \"npm run cypress:headless\"",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe/webhook"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/mdx": "^2.1.1",
    "@sentry/node": "^7.0.0",
    "@sentry/react": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "@stripe/stripe-js": "^1.31.0",
    "@tippyjs/react": "^4.2.6",
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "feed": "^4.2.2",
    "firebase": "^9.8.2",
    "firebase-admin": "^10.2.0",
    "heroicons": "^1.0.6",
    "next": "^12.1.6",
    "next-i18next": "^11.0.0",
    "nodemailer": "^6.7.5",
    "nookies": "^2.5.2",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "raw-body": "^2.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-epic-spinners": "^0.5.0",
    "react-hook-form": "^7.31.3",
    "react-hot-toast": "^2.2.0",
    "reactfire": "^4.2.1",
    "sharp": "^0.30.6",
    "stripe": "^9.6.0",
    "stripe-cli": "^0.1.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.38",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@types/sharp": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.0.2",
    "dotenv": "^16.0.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.1.6",
    "gray-matter": "4.0.3",
    "mjml": "^4.12.0",
    "next-sitemap": "^3.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-highlight": "^5.0.2",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  }
}
